{
  "name": "typescript-template-repository",
  "version": "0.0.0",
  "description": "This is a template for TypeScript projects",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "tslint --project .",
    "start": "node dist/run.js",
    "start:dev": "npm run build && npm run start",
    "test": "c8 mocha dist/tests/*.js --timeout 15000 --exit",
    "test:coverage": "npm run test:coverage-check && npm run test:coverage-report",
    "test:coverage-check": "c8 check-coverage --lines 0 --functions 0 --branches 0",
    "test:coverage-report": "c8 report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SteffenKn/typescript-template-repository.git"
  },
  "keywords": [
    "typescript",
    "template"
  ],
  "author": "Steffen Knaup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SteffenKn/typescript-template-repository/issues"
  },
  "homepage": "https://github.com/SteffenKn/typescript-template-repository#readme",
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.17.1",
    "ip": "1.1.5",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/chai": "4.2.14",
    "@types/cors": "2.8.9",
    "@types/express": "4.17.9",
    "@types/mocha": "8.2.0",
    "@types/node": "14.14.16",
    "@types/node-fetch": "^2.5.7",
    "c8": "7.3.5",
    "chai": "4.2.0",
    "mocha": "8.2.1",
    "tslint": "6.1.3",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "^4.1.3"
  }
}
